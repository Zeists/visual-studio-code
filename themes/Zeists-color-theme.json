{
	"name": "Zeists",
	"colors": {
		// Title Bar
		"titleBar.activeBackground": "#101010",
		"titleBar.border": "#101010",

		// Progress Bar
		"progressBar.background": "#ff0043",

		// Editor
		"editor.background": "#101010",
		"editor.foreground": "#f9ebe0",
		"editor.selectionBackground": "#f9ebe010",
		"editor.selectionHighlightBackground": "#0000",
		"editor.selectionHighlightBorder": "#0000",
		"editorLineNumber.foreground": "#f9ebe030",
