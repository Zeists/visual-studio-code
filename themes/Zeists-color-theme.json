		"tab.hoverBorder": "#FF0043",
		"tab.activeBorder": "#FF0043",
		"tab.dragAndDropBorder": "#ff0043",

		// Menu
		"menu.selectionBackground": "#ff0043",

		// Sidebar
		"sideBar.background": "#101010",
		"sideBar.border": "#101010",
		"sideBar.dropBackground": "#101010",
		"sideBar.foreground": "#f9ebe090",
		"sideBarTitle.foreground": "#0000",
		"list.activeSelectionBackground": "#252525",
		"list.activeSelectionForeground": "#f9ebe0",
		"list.inactiveSelectionForeground": "#f9ebe0",

		// Activity Bar
		"activityBar.background": "#101010",
		"activityBar.activeBorder": "#FF0043",
		"activityBar.foreground": "#f9ebe0",
		"activityBar.inactiveForeground": "#f9ebe040",
		"activityBarBadge.background": "#ff0043",

		 // Input
		"inputOption.activeBackground": "#101010",
		"input.background": "#101010",
		"input.border": "#ff0043",
		"input.foreground": "#f9ebe0",
		"input.placeholderForeground": "#f9ebe040",

		// Button
		"button.background": "#ff0043",
		"button.hoverBackground": "#bb0043",

		// Widgets
		"editorWidget.background": "#101010",
		 "editorWidget.resizeBorder": "#ff0043",
		"debugToolBar.background": "#101010",
		"debugToolBar.border": "#101010",
		"pickerGroup.border": "#101010",
		"pickerGroup.foreground": "#ff0043",
		"widget.border": "#101010",

		// Selection
		"focusBorder": "#0000",
		"selection.background": "#f9ebe030",
		"list.highlightForeground": "#ff0043",

		// Scrollbar
		"scrollbarSlider.activeBackground": "#101010",
		"scrollbarSlider.hoverBackground": "#101010",
		"scrollbarSlider.background": "#00000000",
		"scrollbar.shadow": "#00000000",

		// Status Bar
		"statusBar.debuggingForeground": "#101010",
		"statusBar.background": "#101010",
		"statusBar.focusBorder": "#101010",
		"statusBar.border": "#101010",
		"statusBar.foreground": "#f9ebe0",
		"statusBar.debuggingBackground": "#78BC61",

		// Git Decorations
		"gitDecoration.addedResourceForeground": "#78BC61",
		"gitDecoration.untrackedResourceForeground": "#78BC61",
		"gitDecoration.modifiedResourceForeground": "#FFCB6B",
		"gitDecoration.stageModifiedResourceForeground": "#FFCB6B",
		"gitDecoration.deletedResourceForeground": "#FF5370",
		"gitDecoration.stageDeletedResourceForeground": "#FF5370",
		"gitDecoration.conflictingResourceForeground": "#a10861",
		"gitDecoration.ignoredResourceForeground": "#f9ebe040"
